{
  "build": {
    "content": [
      {
        "src": "source",
        "files": [
          "**/*.md",
          "**/*.yml"
        ]
      }
    ],
    "resource": [
      {
        "src": "source",
        "files": [
          "images/**",
          "favicons/**"
        ]
      }
    ],
    "overwrite": [
      {
        "src": "AUTOGENERATED",
        "files": [
          "*/*.disclaimer.md"
        ]
      }
    ],
    "template": [
      "default",
      "source/template"
    ],
    "dest": "output/site",
    "globalMetadata": {
      "_appTitle": "NUKE",
      "_enableSearch": true,
      "_disableBreadcrumb": true
    },
    "noLangKeyword": false,
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]

  }
}
